  /*********** interview ************/
  /**********************************/
  "interview.updateNotification": false,
  "interview.workspaceFolder": "/Users/albert/CodeSpace/dailyFrontend",
  "javascript.suggest.paths": false,
  /**********************************/
  /********** javascript ************/
  /**********************************/
  "javascript.updateImportsOnFileMove.enabled": "always",
  /**********************************/
  /********** liveServer ************/
  /**********************************/
